{
    "name": "jasmine",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "printWidth": 100
    },
    "scripts": {
        "watch": "bun --watch src/main.ts",
        "compile": "bun src/main.ts",
        "binaryen": "bun compile __.jas --backend binaryen && wasmtime build/wasm/a.wasm",
        "qbe": "bun compile __.jas --backend qbe && ./build/qbe/a.out",
        "executeQbe": "cd build/qbe/ && qbe -o out.s main.ssa && cc out.s && ./a.out"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "binaryen": "^123.0.0"
    }
}
